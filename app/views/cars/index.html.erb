<h1>TRADER AUTO WEBSITE, YO</h1>
<h5>Sassy cars taste the best; enjoy the rainbow!</h5>

<%= will_paginate @cars %>
<table>
	<thead>
		<th>Make</th>
		<th>Model</th>
		<th>Year</th>
	</thead>
	<tbody>
		<% @cars.each do |car| %>
			<tr>
				<td><%= car.make %></td>
				<td><%= car.model %></td>
				<td><%= car.year %></td>
			</tr>
		<% end %>
	</tbody>
</table>

